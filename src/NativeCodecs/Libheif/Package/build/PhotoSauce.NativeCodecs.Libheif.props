<?xml version="1.0" encoding="utf-8"?>
<Project>
	<PropertyGroup Condition="'$(RuntimeIdentifier)' == '' or '$(_UsingDefaultRuntimeIdentifier)' == 'true'">
		<_PackageRuntimesDirectory>$(MSBuildThisFileDirectory)..\..\runtimes\</_PackageRuntimesDirectory>
	</PropertyGroup>

	<ItemGroup Condition="'$(_PackageRuntimesDirectory)' != ''">
		<Content Condition="'$(Platform)' == 'AnyCPU'" Include="$(_PackageRuntimesDirectory)win-x86\native\*" Link="x86\%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" Visible="false" />
		<Content Condition="'$(Platform)' == 'AnyCPU'" Include="$(_PackageRuntimesDirectory)win-x64\native\*" Link="x64\%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" Visible="false" />
		<Content Condition="'$(Platform)' == 'AnyCPU'" Include="$(_PackageRuntimesDirectory)win-arm64\native\*" Link="arm64\%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" Visible="false" />
		<Content Condition="'$(Platform)' == 'x86'" Include="$(_PackageRuntimesDirectory)win-x86\native\*" Link="%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" Visible="false" />
		<Content Condition="'$(Platform)' == 'x64'" Include="$(_PackageRuntimesDirectory)win-x64\native\*" Link="%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" Visible="false" />
		<Content Condition="'$(Platform)' == 'ARM64'" Include="$(_PackageRuntimesDirectory)win-arm64\native\*" Link="%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" Visible="false" />
	</ItemGroup>
</Project>
